<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Application theme. -->
    <style name="AppBaseTheme" parent="android:Theme.Light" />

    <style name="AppTheme" parent="AppBaseTheme" />

    <!-- Dialog -->
    <style name="dialog_general" parent="@android:style/Theme.Dialog">
        <item name="android:textSize">15sp</item>
        <item name="android:backgroundDimEnabled">true</item>        　
        <item name="android:windowIsTranslucent">true</item>        　
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">false</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Holo.Light.Dialog.MinWidth">
        <item name="android:textColor">@color/wms_blue</item>
        <item name="android:textSize">15sp</item>
        <!-- 设置未浮动窗口 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 设置无边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 设置无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置完全透明 -->
        <item name="android:windowBackground">@color/sc_transparent_background</item>
        <!-- 设置屏幕变暗 -->
        <!--<item name="android:backgroundDimEnabled">true</item>
-->
    </style>

    <style name="dialogTheme" parent="@android:style/android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/bg_main</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- RelativeLayout Title -->
    <style name="wms_relativeLayout_title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/wms_blue</item>
    </style>

    <style name="relativeLayout_row">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- LinearLayout样式 主背景 -->
    <style name="wms_linearlayout_main">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">#F0F3F6</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="wms_linearlayout_main_white">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/wms_white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <!-- LinearLayout样式 块 -->
    <style name="wms_linearlayout_block">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:padding">1dp</item>
        <item name="android:background">@drawable/wms_bg_shape</item>
        <item name="android:orientation">vertical</item>
    </style>

    <!-- LinearLayout样式 行 -->
    <!-- 行的共用样式 -->
    <style name="wms_linearlayout_row_base">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="linearlayout_row">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- 行-上圆角样式 -->
    <style name="wms_linearlayout_row_top" parent="wms_linearlayout_row_base">
        <item name="android:background">@drawable/bg_row_top_selector</item>
    </style>

    <!-- 行-无圆角样式  -->
    <style name="wms_linearlayout_row_no" parent="wms_linearlayout_row_base">
        <item name="android:background">@drawable/bg_row_mid_selector</item>
    </style>

    <!-- 行-无圆角样式  -->
    <style name="wms_linearlayout_row_60_no" parent="wms_linearlayout_row_base">
        <item name="android:background">@drawable/bg_row_mid_selector</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <!-- 行-下圆角样式 -->
    <style name="wms_linearlayout_row_bot" parent="wms_linearlayout_row_base">
        <item name="android:background">@drawable/bg_row_bot_selector</item>
    </style>

    <!-- 行-全圆角样式 -->
    <style name="wms_linearlayout_row_all" parent="wms_linearlayout_row_base">
        <item name="android:background">@drawable/bg_row_single_selector</item>
    </style>

    <!-- 行-标签 -->
    <style name="wms_textview_label">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/wms_blue</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wms_textview_label_fill">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/wms_blue</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <!-- 行-TextView值公用 -->
    <style name="wms_textview_value">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/value_color</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <!-- 行-TextView值 左对齐 -->
    <style name="wms_textview_value_left" parent="wms_textview_value">
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <!-- 行-TextView值 右对齐 -->
    <style name="wms_textview_value_right" parent="wms_textview_value">
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <!-- 行-EditView值 -->
    <style name="wms_edittext_value">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:background">@color/wms_white</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:textColor">@color/value_color</item>
    </style>

    <!-- 行-右边箭头样式 -->
    <style name="wms_img_arrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
