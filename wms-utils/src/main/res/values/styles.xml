    <!-- ListView样式 -->
    <style name="wms_listview_common1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:background">@drawable/bg_list</item>
        <item name="android:divider">@color/border_color</item>
        <item name="android:padding">1dp</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:scrollbars">none</item>
        <item name="android:scrollingCache">true</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>

    <!-- ImageView样式 菜单 -->
    <style name="imageview_menu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:contentDescription">@null</item>
    </style>

    <!-- SearchView样式 common1 -->
    <style name="linearlayout_row_search">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/wms_bg_shape</item>
    </style>

    <!-- SearchView样式 common1 -->
    <style name="edittext_search">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@color/wms_white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="searchview_common1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:iconifiedByDefault">false</item>
        <item name="android:inputType">textFilter</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/wms_blue</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="iv_go">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">false</item>
        <item name="android:src">@drawable/go</item>
    </style>

    <!-- ToggleButton -->
    <style name="tb_switch">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:button">@drawable/bg_switch</item>
        <item name="android:checked">false</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:textOn" />
        <item name="android:textOff" />
    </style>

    <style name="radio_button_common">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/wms_blue</item>
        <item name="android:textSize">15sp</item>
    </style>
</resources>